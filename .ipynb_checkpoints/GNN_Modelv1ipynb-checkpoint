     "output_type": "display_data"
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 600x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import torch\n",
    "import torch.nn.functional as F\n",
    "from torch_geometric.loader import DataLoader\n",
    "from torch_geometric.nn import GCNConv, global_mean_pool, BatchNorm\n",
    "from torch_geometric.data import Data\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.utils.class_weight import compute_class_weight\n",
    "from imblearn.over_sampling import SMOTE\n",
    "from imblearn.under_sampling import RandomUnderSampler\n",
    "from sklearn.metrics import confusion_matrix, f1_score\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# 🚀 Carregar os datasets\n",
    "print(\"→ Carregando os datasets...\")\n",
    "df1_full = pd.read_csv(\"Z2JetsToNuNu/Z2JetsToNuNu1_PtZ-150To250.csv\")\n",
    "df2_full = pd.read_csv(\"Z2JetsToNuNu_Negative/ZToNuNu_PtZ-150To250.csv\")\n",
    "\n",
    "df1_full[\"target\"] = 1  \n",
    "df2_full[\"target\"] = 0  \n",
    "\n",
    "df_full = pd.concat([df1_full, df2_full], ignore_index=True)\n",
    "print(f\"→ Dados carregados! Total de eventos: {df_full.shape[0]}\")\n",
    "\n",
    "df_full = df_full.apply(pd.to_numeric, errors=\"coerce\")\n",
    "df_full.fillna(0, inplace=True)\n",
    "\n",
    "undersample = RandomUnderSampler(sampling_strategy=0.4, random_state=42)\n",
    "X_under, y_under = undersample.fit_resample(df_full.drop(columns=[\"target\"]).values, df_full[\"target\"].values)\n",
    "\n",
    "smote = SMOTE(sampling_strategy=0.6, random_state=42)\n",
    "X_bal, y_bal = smote.fit_resample(X_under, y_under)\n",
    "\n",
    "df_bal = pd.DataFrame(X_bal, columns=df_full.columns[:-1])\n",
    "df_bal[\"target\"] = y_bal\n",
    "\n",
    "print(f\"→ Após balanceamento: {df_bal['target'].value_counts().to_dict()}\")\n",
    "\n",
    "jet_columns = [\"lead_jet_pt\", \"lead_jet_eta\", \"lead_jet_phi\", \"lead_jet_mass\"]\n",
    "\n",
    "def build_graph(event):\n",
    "    nodes = [float(event[col]) for col in jet_columns]\n",
    "    edges = [[i, i + 1] for i in range(len(nodes) - 1)]\n",
    "    \n",
    "    x = torch.tensor(nodes, dtype=torch.float).view(-1, 1)\n",
    "    edge_index = torch.tensor(edges, dtype=torch.long).t().contiguous()\n",
    "    \n",
    "    return Data(x=x, edge_index=edge_index, y=torch.tensor([event[\"target\"]], dtype=torch.long))\n",
    "\n",
    "graph_data = [build_graph(row) for _, row in df_bal.iterrows()]\n",
    "train_data, test_data = train_test_split(graph_data, test_size=0.2, random_state=42)\n",
    "\n",
    "train_loader = DataLoader(train_data, batch_size=64, shuffle=True)\n",
    "test_loader = DataLoader(test_data, batch_size=64, shuffle=False)\n",
    "\n",
    "y_train = [data.y.item() for data in train_data]\n",
    "class_weights = compute_class_weight(class_weight=\"balanced\", classes=np.unique(y_train), y=y_train)\n",
    "class_weights = torch.tensor(class_weights, dtype=torch.float)\n",
    "class_weights[1] *= 1.5  # 🔥 Ajustamos para reduzir falsos positivos\n",
    "\n",
    "class GNN(torch.nn.Module):\n",
    "    def __init__(self):\n",
    "        super(GNN, self).__init__()\n",
    "        self.conv1 = GCNConv(1, 16)\n",
    "        self.bn1 = BatchNorm(16)\n",
    "        self.conv2 = GCNConv(16, 32)\n",
    "        self.bn2 = BatchNorm(32)\n",
    "        self.conv3 = GCNConv(32, 64)\n",
    "        self.bn3 = BatchNorm(64)\n",
    "        self.conv4 = GCNConv(64, 16)\n",
    "        self.bn4 = BatchNorm(16)\n",
    "        self.dropout = torch.nn.Dropout(p=0.4)  # 🔥 Dropout reduzido para maior estabilidade\n",
    "        self.fc = torch.nn.Linear(16, 2)\n",
    "\n",
    "    def forward(self, data):\n",
    "        x, edge_index = data.x, data.edge_index\n",
    "        x = self.dropout(F.relu(self.bn1(self.conv1(x, edge_index))))\n",
    "        x = self.dropout(F.relu(self.bn2(self.conv2(x, edge_index))))\n",
    "        x = self.dropout(F.relu(self.bn3(self.conv3(x, edge_index))))\n",
    "        x = self.dropout(F.relu(self.bn4(self.conv4(x, edge_index))))\n",
    "        x = global_mean_pool(x, data.batch) if hasattr(data, \"batch\") else x.mean(dim=0, keepdim=True)\n",
    "        return F.softmax(self.fc(x), dim=1)  # 🔥 Agora usa softmax para prever probabilidades\n",
    "\n",
    "model = GNN()\n",
    "optimizer = torch.optim.Adam(model.parameters(), lr=0.0005, weight_decay=1e-3)\n",
    "scheduler = torch.optim.lr_scheduler.ReduceLROnPlateau(optimizer, mode='max', patience=2, factor=0.5)  \n",
    "criterion = torch.nn.CrossEntropyLoss(weight=class_weights)\n",
    "\n",
    "# 🚀 Treinamento fixado até época 7\n",
    "epochs = 5 \n",
    "accuracy_list, f1_list, loss_list = [], [], []\n",
    "\n",
    "for epoch in range(epochs):\n",
    "    model.train()\n",
    "    total_loss = 0\n",
    "\n",
    "    for data in train_loader:\n",
    "        optimizer.zero_grad()\n",
    "        out = model(data)\n",
    "        loss = criterion(out, data.y)\n",
    "        loss.backward()\n",
    "        torch.nn.utils.clip_grad_norm_(model.parameters(), max_norm=1.0)\n",
    "        optimizer.step()\n",
    "        total_loss += loss.item()\n",
    "\n",
    "    # Avaliação com threshold dinâmico\n",
    "    model.eval()\n",
    "    y_true, y_pred = [], []\n",
    "    with torch.no_grad():\n",
    "        for data in test_loader:\n",
    "            out = model(data)\n",
    "            probs = out[:, 1]  # 🔥 Pegamos apenas a probabilidade da Classe 1\n",
    "            pred = (probs > 0.6).long()  # 🔥 Definimos threshold dinâmico\n",
    "            y_true.extend(data.y.cpu().numpy())\n",
    "            y_pred.extend(pred.cpu().numpy())\n",
    "\n",
    "    acc = np.mean(np.array(y_true) == np.array(y_pred)) * 100\n",
    "    f1 = f1_score(y_true, y_pred, average=\"macro\")\n",
    "    avg_loss = total_loss / len(train_loader)\n",
    "\n",
    "    accuracy_list.append(acc)\n",
    "    f1_list.append(f1)\n",
    "    loss_list.append(avg_loss)\n",
    "\n",
    "    print(f\"→ Época {epoch+1}/{epochs}, Acurácia: {acc:.2f}%, F1-Score: {f1:.4f}, Loss: {avg_loss:.4f}\")\n",
    "\n",
    "print(\"→ Treinamento concluído na época 5!\")\n",
    "\n",
    "# 🚀 Plotando os resultados e salvando\n",
    "plt.figure(figsize=(10, 5))\n",
    "plt.plot(range(1, len(accuracy_list) + 1), accuracy_list, marker='o', linestyle='-', label=\"Acurácia\", color='blue')\n",
    "plt.plot(range(1, len(f1_list) + 1), np.array(f1_list) * 100, marker='s', linestyle='--', label=\"F1-Score (%)\", color='green')\n",
    "plt.xlabel(\"Epoch\")\n",
    "plt.ylabel(\"Accuracy / F1-Score (%)\")\n",
    "plt.title(\"Accuracy and F1-Score Over Training\")\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.savefig(\"accuracy_f1score_v1.png\", dpi=300)\n",
    "plt.show()\n",
    "\n",
    "plt.figure(figsize=(10, 5))\n",
    "plt.plot(range(1, len(loss_list) + 1), loss_list, marker='o', linestyle='-', color='purple', label='Loss')\n",
    "plt.xlabel(\"Epoch\")\n",
    "plt.ylabel(\"Loss (CrossEntropyLoss)\")\n",
    "plt.title(\"Loss Over Training\")\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.savefig(\"loss_training_v1.png\", dpi=300)\n",
    "plt.show()\n",
    "\n",
    "cm = confusion_matrix(y_true, y_pred)\n",
    "cm_df = pd.DataFrame(cm, index=[\"Classe 0\", \"Classe 1\"], columns=[\"Previsto 0\", \"Previsto 1\"])\n",
    "plt.figure(figsize=(6, 5))\n",
    "sns.heatmap(cm_df, annot=True, fmt=\"d\", cmap=\"Blues\", cbar=False)\n",
    "plt.title(\"Confusion Matrix\")\n",
    "plt.ylabel(\"True Label\")\n",
    "plt.xlabel(\"Predicted Label\")\n",
    "plt.savefig(\"confusion_matrix_v1.png\", dpi=300)\n",
    "plt.show()\n"
   ]
  },
